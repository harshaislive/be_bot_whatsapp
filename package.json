{
  "name": "enterprise-whatsapp-bot",
  "version": "1.0.0",
  "description": "Enterprise-grade WhatsApp bot using BuilderBot and Azure OpenAI",
  "main": "src/app.js",
  "type": "module",
  "scripts": {
    "start": "node scripts/start.js",
    "start:prod": "NODE_ENV=production node src/app-enterprise.js",
    "dev": "node --watch src/app.js",
    "setup": "node scripts/setup.js",
    "build": "node build.js",
    "build:docker": "docker build -t whatsapp-bot .",
    "deploy:coolify": "echo 'Deploy using git push to your Coolify remote'",
    "test": "jest",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "logs": "tail -f logs/app.log",
    "status": "node scripts/status.js",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "builderbot",
    "ai",
    "enterprise",
    "azure",
    "openai"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@builderbot/bot": "^1.3.4",
    "@builderbot/database-json": "^1.3.4",
    "@builderbot/provider-baileys": "^1.3.4",
    "@supabase/supabase-js": "^2.57.4",
    "@whiskeysockets/baileys": "^7.0.0-rc.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.21.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.0",
    "morgan": "^1.10.0",
    "openai": "^4.0.0",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "rate-limiter-flexible": "^2.4.0",
    "redis": "^5.8.2",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "jest": "^29.6.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
